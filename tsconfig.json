{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "noEmit": true,
    "inlineSources": true,
    "sourceMap": true,
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules/"]
}
