{
  "extends": "./tsconfig",
  "compilerOptions": {
    "noEmit": false,
    "target": "esnext",
    "module": "CommonJS",
    "outDir": "./dist",
    "declaration": true,
    "inlineSources": true,
    "sourceMap": true,
